  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.kh.ex02.UserMapper">

	<select id="login" resultType="UserVo">
		select * from tbl_user
		where u_id = #{u_id}
		and u_pw = #{u_pw}
	</select>
